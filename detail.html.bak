<script>
    async function fetchImage2() {
        try {
            const urlParams = new URLSearchParams(window.location.search);
            const objectId = urlParams.get('objectId');

            const response = await fetch(`https://lazy-cyan-jellyfish-garb.cyclic.app/blog/${objectId}`);
            const image = await response.json();

            const blogDetails = document.getElementById('blogDetails');

            blogDetails.innerHTML += `     
					<div class="mb-5">
                        <img class="img-fluid w-100 rounded mb-5" src="${image.url}" alt="">
                        <h1 class="mb-4">${image.title}</h1>
                        <p>${image.text}.</p>
                    </div>
`;
        } catch (error) {
            console.error('Error fetching image:', error);
        }
    }

    fetchImage2();
</script>
<script>
   async function fetchImages() {
  try {
    const response = await fetch('https://lazy-cyan-jellyfish-garb.cyclic.app/images');
    const data = await response.json();

    const recentPosts = document.getElementById('recentPosts');
	
    data.forEach(image => {
						
						
	 recentPosts.innerHTML += `<div class="d-flex rounded overflow-hidden mb-3" style="background:#fef1ef;">
                            <img class="img-fluid" src="${image.url}" style="width: 100px; height: 100px; object-fit: cover;" alt="">
                            <a href="" class="h5 fw-semi-bold d-flex align-items-center px-3 mb-0" style="background:#fef1ef;">${image.text}
                            </a>
                        </div>`;
    });
  } catch (error) {
    console.error('Error fetching images:', error);
  }
}

    fetchImages();
</script>





